{
  "version": 2,
  "buildCommand": "next build",
  "devCommand": "next dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "builds": [
    {
      "src": "second-souffle/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "second-souffle/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_BASE_URL": "https://second-souffle.vercel.app"
  }
}
